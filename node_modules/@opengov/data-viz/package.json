{
  "name": "@opengov/data-viz",
  "version": "1.1.0",
  "description": "fn React SVG library",
  "repository": "https://github.com/OpenGov/open-data-viz",
  "license": "MIT",
  "main": ".build/index.js",
  "files": [
    ".build/"
  ],
  "devDependencies": {
    "@storybook/addon-knobs": "3.4.1",
    "@storybook/addons": "3.4.1",
    "@storybook/react": "3.4.1",
    "@types/d3-collection": "1.0.7",
    "@types/d3-interpolate": "1.3.0",
    "@types/d3-scale": "2.0.0",
    "@types/d3-shape": "1.2.7",
    "@types/jest": "23.3.12",
    "@types/lodash": "4.14.119",
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "@types/storybook__react": "3.0.7",
    "awesome-typescript-loader": "4.0.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "23.4.2",
    "jest-cli": "23.4.2",
    "jest-css-modules": "1.1.0",
    "jest-mock": "23.2.0",
    "jest-util": "23.4.0",
    "node-sass": "4.8.3",
    "react-test-renderer": "16.4.1",
    "sass-loader": "6.0.7",
    "style-loader": "0.21.0",
    "ts-jest": "23.0.1",
    "tslint": "5.11.0",
    "tslint-microsoft-contrib": "5.2.1",
    "tslint-react": "3.5.1",
    "typescript": "3.2.2",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "2.11.1"
  },
  "dependencies": {
    "color": "3.1.0",
    "d3-array": "1.2.1",
    "d3-collection": "1.0.7",
    "d3-format": "1.2.2",
    "d3-interpolate": "1.3.2",
    "d3-scale": "2.0.0",
    "d3-shape": "1.2.2",
    "lodash.isempty": "4.4.0",
    "lodash.max": "4.0.1",
    "lodash.min": "4.0.1",
    "lodash.times": "4.3.2",
    "lodash.uniq": "4.5.0",
    "react": "16.4.1",
    "react-dom": "16.4.2"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.s?css$": "<rootDir>/node_modules/jest-css-modules"
    },
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "<rootDir>/src/**"
    ],
    "coveragePathIgnorePatterns": [
      "/stories/",
      "-book.tsx",
      "/examples/",
      ".css",
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 58,
        "functions": 70,
        "lines": 85,
        "statements": 83
      }
    },
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jest.js"
  },
  "scripts": {
    "build": "webpack -p --progress --colors",
    "build:dist": "webpack -p --progress --colors --config ./scripts/react.webpack.config.js",
    "prepublishOnly": "yarn build:dist",
    "coverage": "jest --coverage",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "tableflip": "scripts/tableflip.sh",
    "clean": "npm run tableflip",
    "lint": "tslint -c tslint.json './src/**/*.ts*' './index.ts'"
  }
}
